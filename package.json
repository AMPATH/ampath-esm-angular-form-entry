{
  "name": "ampath-esm-angular-form-entry",
  "version": "1.0.0-SNAPSHOT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run build:single-spa-local",
    "test": "ng test --watch false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:single-spa-local": "ng build --prod --output-hashing=none --deploy-url https://localhost:4202/ && npm run rename:mainjs",
    "build:single-spa-travis": "bash ./scripts/build-travis.sh",
    "serve:single-spa": "ng serve --ssl true  --disable-host-check --deploy-url https://localhost:4202/ --host localhost --port 4202 --live-reload true",
    "serve:dist": "npx angular-http-server -p 4202 --https --cors --path dist/ampath-esm-angular-form-entry/",
    "build-serve:dist": "npm run build:single-spa-local && npm run serve:dist",
    "rename:mainjs": "node -e \"require('fs').rename('dist/ampath-esm-angular-form-entry/main-es2015.js', 'dist/ampath-esm-angular-form-entry/ampath-esm-form-entry.js', function(err) { if (err) console.log(err); console.log('File successfully renamed!') })\""
  },
  "private": true,
  "dependencies": {
    "@ampath-kenya/ngx-openmrs-formentry": "^3.0.0-alpha3",
    "@angular-builders/custom-webpack": "^8",
    "@angular/animations": "~8.2.14",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/forms": "~8.2.14",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/router": "~8.2.14",
    "@openmrs/esm-api": "^1.2.1",
    "ngx-bootstrap": "next",
    "ngx-file-uploader": "0.0.18",
    "rxjs": "^6.5.3",
    "single-spa-angular": "^3.1.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.20",
    "@angular/cli": "~8.3.20",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^8.9.5",
    "angular-http-server": "^1.9.0",
    "codelyzer": "^5.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3",
    "webpack-bundle-analyzer": "^3.6.0"
  }
}
